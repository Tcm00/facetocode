    </select>
    <select id="getRiskInfo" resultType="com.ming.facetocode.pojo.DO.RiskInfoDO">
        select * from riskInfo
    </select>
    <select id="getTripInfo" resultType="com.ming.facetocode.pojo.DO.TripInfoDO">
        select * from tripInfo
    </select>
