        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ming.facetocode.mapper.TableMapper">


    <select id="getAdminUser" resultType="com.ming.facetocode.pojo.DO.AdminUserDO">
